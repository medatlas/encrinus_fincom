{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [
      "node"
    ],
    "typeRoots": [
      "../node_modules/@types"
    ],
    "paths": {
      "stream": [
        "node_modules/stream-browserify"
      ],
      "@angular/*": [
        "node_modules/@angular/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}